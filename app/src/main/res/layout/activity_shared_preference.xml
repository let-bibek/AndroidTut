<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btnSharedLogout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SharedPreference">

    <Button
        android:id="@+id/btnSharedPreference"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/butSharedLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btnSharedPreference"
        app:layout_constraintStart_toStartOf="@+id/btnSharedPreference"
        app:layout_constraintTop_toBottomOf="@+id/btnSharedPreference" />

</androidx.constraintlayout.widget.ConstraintLayout>